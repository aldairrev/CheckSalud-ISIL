<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.7.2" />
    </startup>
    <system.serviceModel>
        <bindings>
            <basicHttpBinding>
                <binding name="BasicHttpBinding_IServicioResultado" />
                <binding name="BasicHttpBinding_IServicioAnalisis" />
                <binding name="BasicHttpBinding_IServicioEvaluacion" />
            </basicHttpBinding>
        </bindings>
        <client>
            <endpoint address="http://localhost:8733/Design_Time_Addresses/WCF_CheckSalud/ServicioResultado/"
                binding="basicHttpBinding" bindingConfiguration="BasicHttpBinding_IServicioResultado"
                contract="ProxyResultado.IServicioResultado" name="BasicHttpBinding_IServicioResultado" />
            <endpoint address="http://localhost:8733/Design_Time_Addresses/WCF_CheckSalud/ServicioAnalisis/"
                binding="basicHttpBinding" bindingConfiguration="BasicHttpBinding_IServicioAnalisis"
                contract="ProxyAnalisis.IServicioAnalisis" name="BasicHttpBinding_IServicioAnalisis" />
            <endpoint address="http://localhost:8733/Design_Time_Addresses/WCF_CheckSalud/ServicioEvaluacion/"
                binding="basicHttpBinding" bindingConfiguration="BasicHttpBinding_IServicioEvaluacion"
                contract="ProxyEvaluacion.IServicioEvaluacion" name="BasicHttpBinding_IServicioEvaluacion" />
        </client>
    </system.serviceModel>
</configuration>